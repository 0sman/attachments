    "files": [
        "index.cds",
        "lib",
        "srv"
    ],
    "devDependencies": {
        "@cap-js/sqlite": "^1.0.1",
        "axios": "^1.4.0",
        "eslint": "^8",
        "express": "^4.18.2",
        "jest": "^29.5.0"
    },
    "peerDependencies": {
        "@sap/cds": "^7"
    },
    "cds": {
